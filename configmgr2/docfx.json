{
  "build": {
    "overwrite": [],
    "externalReference": [],
    "fileMetadata": {},
    "template": [],
    "dest": "configmgr2",
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
